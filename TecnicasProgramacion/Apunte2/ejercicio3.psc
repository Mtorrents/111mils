Proceso ejercicio3
	Definir genero Como Caracter;
	Escribir 'Ingrese su genero: ';
	Leer genero;
	Si genero='Masculino' O genero='Femenino' Entonces
		Si genero='Femenino' Entonces
			Escribir 'El genero ingresado es Femenino';
		FinSi
	SiNo
		Escribir 'El genero ingresado no es valido';
	FinSi
FinProceso

